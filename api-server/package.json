{
	"name": "api-server",
	"version": "1.0.0",
	"description": "cocode api service",
	"main": "app.js",
	"scripts": {
		"dev": "NODE_ENV=development nodemon --exec babel-node src/app.js",
		"start": "NODE_ENV=production babel-node src/app.js",
		"test": "jest"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm test && lint-staged",
			"pre-push": "npm test",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"lint-staged": {
		"*.js": [
			"eslint --fix",
			"git add"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/connect-foundation/2019-04"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/connect-foundation/2019-04/issues"
	},
	"homepage": "https://github.com/connect-foundation/2019-04#readme",
	"dependencies": {
		"@babel/runtime": "^7.6.2",
		"cookie-parser": "~1.4.4",
		"cors": "^2.8.5",
		"debug": "~2.6.9",
		"dotenv": "^8.2.0",
		"express": "~4.16.1",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^5.7.9",
		"passport": "^0.4.0",
		"passport-github": "^1.1.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.6.2",
		"@babel/core": "^7.6.2",
		"@babel/node": "^7.6.2",
		"@babel/plugin-transform-runtime": "^7.6.2",
		"@babel/polyfill": "^7.6.0",
		"@babel/preset-env": "^7.6.2",
		"@commitlint/cli": "^8.2.0",
		"@commitlint/config-conventional": "^8.2.0",
		"eslint": "^6.6.0",
		"eslint-config-prettier": "^6.5.0",
		"eslint-plugin-react": "^7.16.0",
		"husky": "^3.0.9",
		"jest": "^24.9.0",
		"lint-staged": "^9.4.2",
		"nodemon": "^1.19.0",
		"prettier": "^1.19.1"
	}
}
